<%- partial('top') %> 

<form class="col-xs-6 form-inline pull-right" action="/user/show_deleteds" method="GET">
	<div class="form-group">
	<label for="search">Search for Username</label>
	<input  class="form-control" type="text" name="search">
	</div>

  <div class="form-group">
	<input type="submit" class="btn btn-primary" value="search"/>
	</div>
</form>

<table class="table table-hover">
	<thead class="thead-inverse">
	<tr>
		<th>Username</th>
		<th>Email</th>
		<th>User Level</th>
		<th>Created At</th>
		<th>Removed At</th>
	</tr>
	</thead>
	<tbody>
	<% users.forEach( function( model ){ %>
	<tr>
		<td><%= model.name %></td>
		<td><%= model.email %></td>
		<td><%= model.userlevel %></td>
		<td><%= model.createdAt %></td>
		<td><%= model.updatedAt %></td>

	</tr>

	<% }); %>
	</tbody>
</table>

<a href="/user/index">Back</a>

<%- partial('bottom') %> 
